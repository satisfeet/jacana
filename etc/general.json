{

    "name": "satisfeet-store",

    "static": {
        "path": "/srv",
        "passthrough": true
    },

    "engine": {
        "views": {
            "path": "/var/views"
        },
        "styles": {
            "src": "/var/styles",
            "dest": "/srv/stylesheets",
            "prefix": "/stylesheets"
        },
        "builder": {
            "entries": [
                "/app/index.js"
            ],
            "output": "/srv/javascripts/build.js",
            "aliases": {
                "views": "./var/views/app"
            }
        }
    }

}
