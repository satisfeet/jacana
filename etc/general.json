{

  "name": "satisfeet-store",

  "static": {
    "path": "/srv",
    "index": false,
    "passthrough": true
  },

  "engine": {
    "views": {
      "path": "/var/views"
    },
    "styles": {
      "src": "/var/styles",
      "dest": "/srv/stylesheets",
      "prefix": "/stylesheets"
    },
    "builder": {
      "entries": [
        "/app/index.js"
      ],
      "aliases": {
        "views": "../var/views/app"
      },
      "templates": {
        "compress": true
      },
      "output": "/srv/javascripts/build.js",
      "extensions": [
        ".js",
        ".json",
        ".html"
      ]
    }
  }

}
