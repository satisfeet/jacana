{

  "name": "satisfeet-store",

  "static": {
    "path": "/srv",
    "passthrough": true
  },

  "engine": {
    "views": {
      "path": "/usr/views",
      "engine": "jade"
    },
    "styles": {
      "src": "/usr/styles",
      "dest": "/srv/stylesheets",
      "prefix": "/stylesheets"
    }
  }

}
